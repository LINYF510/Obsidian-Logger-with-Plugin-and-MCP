{
  "log_file_path": "/path/to/your/vault/../obsidian-logger/obsidian-debug.log",
  "vault_path": "/path/to/your/vault",
  "cache_size": 1000,
  "cache_ttl_seconds": 300,
  "file_monitor": {
    "enabled": true,
    "debounce_ms": 100
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
  }
}
